[:div {:title "index-notifications"}
 [:h1
  [:span "Index Notifications"]
  [:button.btn.pull-right {:ng-click "refresh()"} "Refresh"]]

 [:debug {:expr "page"}]
 [:div {:ng-if "!loaded"} [:spinner]]
 [:div {:ng-if "loaded"}
  [:div {:ng-repeat "id in page.items"}
   [:debug {:expr "id"}]
   [:show-notification.well {:data-id "{{id}}"}]]]]
