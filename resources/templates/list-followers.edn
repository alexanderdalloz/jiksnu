[:div
 [:h3 "Followers"]
 [:subpage {:name "followers" :item "item"}
  [:button.btn {:ng-click "$parent.refresh()"} "Refresh"]
  [:debug {:expr "$parent.page"}]
  [:ul
   [:li {:ng-repeat "follow in $parent.page.items"}
    [:p
     [:display-avatar {:data-id "{{follow.from}}"}]
     [:a {:ui-sref "showUser({_id: follow.from})"}
      "{{follow.from}}"]]
    [:debug {:expr "follow"}]]]]]
