[:div
 [:h2 "Index Users"]
 [:div {:ng-if "!loaded"}
  [:p "Loading"]]
 [:div {:ng-if "loaded"}
  [:table.table.users
   [:thead]
   [:tbody
    [:tr {:ng-repeat "user in page.items"}
     [:td
      [:display-avatar {:data-id "{{user._id}}"}]
      [:p
       [:link-to {:data-id "{{user._id}}" :data-model "User"}]]
      [:p "{{user.username}}@{{user.domain}}"]
      [:p "{{user._id}}"]
      [:p "{{user.name}}"]
      [:p "{{user.uri}}"]
      [:p "{{user.bio}}"]]]]]]]
