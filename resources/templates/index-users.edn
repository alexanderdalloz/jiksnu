[:div {:title "index-users"}
 [:h1
  [:span "Users"]
  [:button.btn.pull-right {:ng-click "refresh()"} "Refresh"]]
 [:div {:ng-if "!loaded"} [:spinner]]
 [:div {:ng-if "loaded"}
  [:debug {:expr "page"}]
  [:ul
   [:li {:ng-repeat "id in page.items" :title "index-users repeat"}
    [:debug {:expr "id"}]
    [:show-user-minimal {:data-id "{{id}}"}]]]]]
