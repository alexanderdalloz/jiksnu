[:div {:title "list-groups"}
 [:h3 "Groups"  [:button.btn.pull-right {:ng-click "refresh()"} "Refresh"]]
 [:subpage {:name "groups" :item "item"}
  [:debug {:expr "$parent.page"}]
  [:div {:ng-if "!$parent.loaded"} [:spinner]]
  [:div {:ng-if "$parent.loaded"}
   [:ul
    [:li {:ng-repeat "id in $parent.$parent.page.items"}
     [:debug {:expr "id"}]
     [:show-group-minimal {:data-id "{{id}}"}]]]]]]
