[:div
 [:h3 "Following"]
 [:subpage {:name "following" :item "item"}
  [:debug {:expr "$parent.page"}]
  [:ul
   [:li {:ng-repeat "follow in $parent.page.items"}
    [:debug {:expr "follow"}]
    [:display-avatar {:data-id "{{follow.to}}"}]
    [:a {:ui-sref "showUser({_id: follow.to})"}
     "{{follow.to}}"]]]]]
